{
    "manifest_version": 2,
    "name": "Web Page Question Answering",
    "version": "1.0",
    "description": "A Chrome extension that performs question answering over the full text of a web page using the Superpowered API.",
    "icons": {
      "48": "icon.png"
    },
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Web Page Question Answering"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}